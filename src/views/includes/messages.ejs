<% if(success.length > 0) { %>
    <div class="success">
        <% success.forEach(success => { %>
        <%= success %><br>
        <% }); %>
    </div>
<% } %>

<% if(errors.length > 0){ %>
    <% errors.forEach(erro => { %>
        <div class="errors">
            <%= erro %><br>
        </div>
    <% }); %>
<% } %>